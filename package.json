{
  "name": "nodejs-express-starter",
  "version": "1.0.0",
  "description": "Node.js Express starter with MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "node ./server.js",
    "test": "npm run lint && npm run jest -i",
    "jest": "jest -i --coverage --ci",
    "lint": "eslint ."
  },
  "jest": {
    "testEnvironment": "node",
    "bail": true,
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "/libs/",
      "/middlewares/",
      "/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salikovpro/nodejs-express-starter.git"
  },
  "keywords": [
    "Node.js",
    "Express",
    "MongoDB"
  ],
  "author": "Iakov Salikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/salikovpro/nodejs-express-starter/issues"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm test"
    }
  },
  "homepage": "https://github.com/salikovpro/nodejs-express-starter#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "colors": "^1.1.2",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.2",
    "express-useragent": "^1.0.12",
    "jest-environment-node": "^22.3.0",
    "joi": "^13.1.2",
    "moment": "^2.20.1",
    "mongoose": "^5.0.4",
    "mongoose-double": "0.0.1",
    "morgan": "^1.9.0",
    "nconf": "^0.10.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "ghooks": "^2.0.2",
    "jest": "^22.3.0",
    "superagent": "^3.8.2",
    "supertest": "^3.0.0"
  }
}
